import java.io.*;

class FileOutputStreamWrite  
{
	public static void main(String[] args)  throws Exception
	{

		//choose file
		String fname = args[0];

		//open in write mode
		FileOutputStream fos = new FileOutputStream(fname,false);
		//wrte Operations

	for(int i=1;i<=10;i++)
		{
		System.out.println("i	value"+i);
		fos.write(i);
		}
		
	}
}
